{
    "name": "vscode-galah-syntax",
    "displayName": "Galah Language",
    "description": "Galah Language Extension by the Wabi Foundation",
    "version": "0.0.1",
    "author": {
        "name": "furbyâ„¢",
        "email": "devs@wabi.foundation",
        "url": "https://github.com/wabiverse"
    },
    "publisher": "wabifoundation",
    "keywords": [
        "wabifoundation",
        "wabi",
        "galah",
        "swift"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/wabiverse/galah_vscode_extension"
    },
    "icon": "images/wabifoundation_logo.jpg",
    "engines": {
        "vscode": "^1.34.0"
    },
    "main": "src/extension.js",
    "activationEvents": [
        "onLanguage:galah"
    ],
    "devDependencies": {
        "@types/node": "^8.10.51",
        "@types/vscode": "^1.34.0",
        "vscode-uri": "^2.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "galah",
                "aliases": [
                    "Galah",
                    "galah"
                ],
                "extensions": [
                    ".galah",
                    ".galah.in"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "galah",
                "scopeName": "source.galah",
                "path": "./syntaxes/galah.tmLanguage.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Galah"
        }
    }
}
